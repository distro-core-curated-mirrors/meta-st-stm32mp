+/* HELPER: search detected driver */
+struct detect_info_t {
+	bool (*detect)(void);
+	char *compatible;
+};
+
+static const char *detect_device(const struct detect_info_t *info, u8 size)
+{
+	u8 i;
+
+	for (i = 0; i < size; i++) {
+		if (info[i].detect())
+			return info[i].compatible;
+	}
+
+	return NULL;
+}
