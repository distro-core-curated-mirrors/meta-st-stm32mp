+	hwdata = (struct stm32mp2_usb2phy_hw_data *)dev_get_driver_data(dev);
+	if (!hwdata)
+		return NULL;
+	for (i = 0; (hwdata[i].cr_offset != offset) && hwdata[i].cr_offset; i++)
